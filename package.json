{
  "name": "fastify-next",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "debug": "node --inspect node_modules/.bin/next",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:web": "BUNDLE_ANALYZE=web npm run build",
    "analyze:all": "BUNDLE_ANALYZE=all npm run build",
    "predeploy": "rimraf node_modules/.cache && next build && next export && touch out/.nojekyll && touch out/CNAME && echo \"leolefevre.photography\" >> out/CNAME",
    "deploy": "git add out/ && git commit -m \"Deploy to gh-pages\" && git push origin `git subtree split --prefix out master`:gh-pages --force"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fastify": "^1.13.4",
    "fastify-nextjs": "^4.0.0",
    "next": "^7.0.2",
    "next-fonts": "^0.16.0",
    "polished": "^2.3.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-define": "^1.3.1",
    "next-images": "^1.0.4",
    "push-dir": "^0.4.1",
    "rimraf": "^2.6.3"
  }
}
